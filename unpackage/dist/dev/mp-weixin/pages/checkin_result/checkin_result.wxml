<view class="page"><view class="summary-container"><view class="user-info"><image src="{{a}}" mode="widthFix" class="photo"></image><view class="info"><text class="username">{{b}}</text><text class="dept">隶属部门：{{c}}</text></view></view><view class="date">{{d}}</view></view><view class="result-container"><view class="left"><image src="{{e}}" mode="widthFix" class="icon-timer"></image><image src="{{f}}" mode="widthFix" class="icon-timer"></image><view class="line"></view></view><view class="right"><view class="row"><text class="start">上班（时间 {{g}}）</text></view><view class="row"><text class="checkin-time">签到时间（{{h}}）</text><text wx:if="{{i}}" class="checkin-result green other">{{j}}</text><text wx:if="{{k}}" class="checkin-result yellow other">{{l}}</text></view><view class="row"><image src="{{m}}" mode="widthFix" class="icon-small"></image><text class="desc">{{n}}</text><text wx:if="{{o}}" class="checkin-result green">{{p}}</text><text wx:if="{{q}}" class="checkin-result yellow">{{r}}</text><text wx:if="{{s}}" class="checkin-result red">{{t}}</text></view><view class="row"><image src="{{v}}" mode="widthFix" class="icon-small"></image><text class="desc">身份验证</text><text class="checkin-result green">已通过</text></view><view class="row"><text class="end">下班（时间 {{w}}）</text></view></view></view><view class="checkin-report"><image src="{{x}}" mode="widthFix" class="big-icon"></image><view class="report-title"><text class="days">{{y}}</text><text class="unit">天</text></view><view class="sub-title"><text>累计签到</text><view class="line"></view></view><view class="calendar-container"><view wx:for="{{z}}" wx:for-item="one" wx:key="l" class="calendar"><image wx:if="{{one.a}}" src="{{one.b}}" mode="widthFix" class="calendar-icon"></image><image wx:if="{{one.c}}" src="{{one.d}}" mode="widthFix" class="calendar-icon"></image><text class="day">{{one.e}}</text><text wx:if="{{one.f}}" class="result green">{{one.g}}</text><text wx:if="{{one.h}}" class="result yellow">{{one.i}}</text><text wx:if="{{one.j}}" class="result red">{{one.k}}</text></view></view></view></view>